# Next.js production environment for AWS EC2
# Copy to nextjs/.env.production and set the values before the first deploy.

# Core URLs
NEXT_PUBLIC_DIRECTUS_URL=https://directus.exclusivalarimoveis.com.br
NEXT_PUBLIC_SITE_URL=https://exclusivalarimoveis.com.br

# Directus tokens
DIRECTUS_ADMIN_TOKEN=STATIC_DIRECTUS_ADMIN_TOKEN
DIRECTUS_PUBLIC_TOKEN=PUBLIC_TOKEN_FOR_PUBLIC_CONTENT
DIRECTUS_FORM_TOKEN=FORM_TOKEN_FOR_FORMS

# Preview & editor features
DRAFT_MODE_SECRET=STRONG_DRAFT_MODE_SECRET
NEXT_PUBLIC_ENABLE_VISUAL_EDITING=false
NEXT_PUBLIC_DEFAULT_THEME_KEY=ulm
